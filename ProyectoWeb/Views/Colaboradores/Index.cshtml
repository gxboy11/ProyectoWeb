@model IEnumerable<ProyectoWeb.Models.Colaborador>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cedula_colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre_colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellidos_colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correo_colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contrasena_colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefono_colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion_colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salario_colaborador)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cedula_colaborador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre_colaborador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellidos_colaborador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correo_colaborador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contrasena_colaborador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono_colaborador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion_colaborador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salario_colaborador)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
